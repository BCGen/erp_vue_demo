{
  /**************
   * Text Editor
   **************/
  // "editor.codeActionsOnSave": {
  // "source.fixAll": true
  // },
  "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "liveshare.nameTagVisibility": "Never",
  "editor.lineHeight": 40,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.snippetSuggestions": "inline",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "files.autoSave": "afterDelay",
  /************
     * Workbench
     ************/
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "material-icon-theme",
  /*********
     * Window
     *********/
  // "window.menuBarVisibility": "hidden",
  "window.zoomLevel": 0,
  /***********
     * Features
     ***********/
  "explorer.openEditors.visible": 0,
  "terminal.integrated.shell.windows": "cmd.exe",
  "terminal.integrated.shellArgs.windows": [
    "/k",
    "C:\\tools\\cmder\\vendor\\init.bat"
  ],
  "terminal.external.windowsExec": "C:\\tools\\cmder\\cmder.exe",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.external.osxExec": "iTerm.app",
  /*********************
     * Extensions
     *********************/
  // Code Settings Sync
  "sync.forceUpload": true,
  "sync.gist": "1e9ba505cce9b43c97ad4a776b6bc701",
  "sync.quietSync": true,
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  // Emmet
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue": "html"
  },
  // Eslint
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  // "eslint.validate": [
  //   "javascript",
  //   "javascriptreact",
  //   "typescript",
  // "vue"
  // ],
  // Html
  "html.format.wrapAttributes": "aligned-multiple",
  "html.format.wrapLineLength": 0,
  "html.format.indentInnerHtml": true,
  // TypeScript and JavaScript
  "javascript.referencesCodeLens.enabled": false,
  // PHP
  "php.suggest.basic": false,
  "php.validate.enable": false,
  // Prettier
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.htmlWhitespaceSensitivity": "ignore",
  // Project Manager
  "projectManager.any.maxDepthRecursion": 1,
  "projectManager.any.baseFolders": [
    "/Users/bc/Documents/practices",
    "/Users/bc/Documents/projects"
  ],
  "projectManager.git.baseFolders": [
    "/Users/bc/Documents/practices",
    "/Users/bc/Documents/projects"
  ],
  // Stylelint
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  // Vetur
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.experimental.templateInterpolationService": true,
  "vetur.validation.style": false,
  // "vetur.validation.template": false,
  // "vetur.validation.script": false,
  // Vim
  "vim.easymotion": true,
  "vim.easymotionMarkerFontSize": "18",
  "vim.easymotionMarkerHeight": 30,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-c>": false,
    "<C-d>": false,
    "<C-f>": false,
    "<C-t>": false,
    "<C-v>": false,
    "<C-x>": false
  },
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "a"
      ],
      "commands": [
        "editor.action.triggerSuggest"
      ]
    },
    {
      "before": [
        "<leader>",
        "c"
      ],
      "commands": [
        "postcssSorting.execute",
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<leader>",
        "d"
      ],
      "commands": [
        "code-runner.run"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "g"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    {
      "before": [
        "<leader>",
        "k",
        "g"
      ],
      "commands": [
        "workbench.action.closeEditorsInGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "i",
        "c"
      ],
      "commands": [
        "phpConstructor.insert"
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "commands": [
        "extension.liveServer.goOnline"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "commands": [
        "workbench.actions.view.problems"
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "s"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<leader>",
        "v"
      ],
      "commands": [
        "extension.vim_ctrl+v"
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "<leader>",
        ","
      ],
      "commands": [
        "workbench.action.openSettings"
      ]
    }
  ],
  // Code Spell Checker
  "cSpell.userWords": [
    "Autoloading",
    "Autosize",
    "Bcgen",
    "Codedatas",
    "Consolas",
    "DEBUGMODE",
    "Dropdown",
    "ERRMODE",
    "Fira",
    "Interop",
    "LANGABILITY",
    "LANGCONDITION",
    "Laravel",
    "Minification",
    "Multiselect",
    "Nuxt",
    "Popconfirm",
    "Preprocessors",
    "QRCODE",
    "Resumefiles",
    "SMTP",
    "Spatie",
    "Submenu",
    "Swiper",
    "Uploader",
    "Vuetify",
    "allowfullscreen",
    "antd",
    "appstore",
    "arraybuffer",
    "asynciterable",
    "autoload",
    "backend",
    "cmder",
    "comefrom",
    "cond",
    "coverflow",
    "createdate",
    "crossorigin",
    "csrf",
    "devtool",
    "easymotion",
    "editorconfig",
    "emsp",
    "endforeach",
    "esbenp",
    "esnext",
    "frameborder",
    "fromname",
    "fromstr",
    "gapi",
    "getmail",
    "gitignore",
    "gmail",
    "grecaptcha",
    "keyframes",
    "maildatapath",
    "mainbody",
    "middlewares",
    "mixins",
    "monospace",
    "mytodo",
    "nums",
    "nuxtjs",
    "parameterd",
    "parameterm",
    "paren",
    "productpages",
    "repo",
    "ruker",
    "serviceissue",
    "servicelog",
    "singleline",
    "stylelint",
    "stylelintrc",
    "tailwindcss",
    "todos",
    "usebcc",
    "vetur",
    "vuex",
    "webp",
    "wpid"
  ],
  "editor.quickSuggestions": {
    "strings": true
  },
  "rest-client.enableCustomVariableReferencesCodeLens": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.port": 6600,
